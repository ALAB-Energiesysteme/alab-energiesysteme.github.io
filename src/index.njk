<script>
  document.addEventListener('DOMContentLoaded', () => {
    // Alle Links, die einen neuen Tab erzwingen, zurück auf Standard
    document.querySelectorAll('a[target="_blank"]').forEach(a => {
      a.removeAttribute('target');
      if (a.hasAttribute('rel')) a.removeAttribute('rel');
    });

    // Falls irgendwo window.open(...) in onclick genutzt wird → im selben Tab öffnen
    document.querySelectorAll('[onclick*="window.open"]').forEach(el => {
      const handler = el.getAttribute('onclick');
      if (!handler) return;
      el.setAttribute(
        'onclick',
        handler.replace(/window\.open\(([(^)]*)\)/g, 'window.location.href=$1')
      );
    });
  });
</script>


